{
  "CsvPath": "$baseDir/CSVData/data.csv",
  "HasHeader": true,

  "StylePath": "$baseDir/Configurations/Style.json",
  "PreviewPath": "$baseDir/CSVData/<Train>.png",
  "CopyRowFormat": "<Train>: <Top Speed> km/h",

  "UseInputAlias": true,
  "UseOutputAlias": false,
  "UseRegexFilter": false,
  "UseToolTip": true,

  "CanEdit": true,
  "AllowMultiline": true,
  "InsertSelectedCount": true,
  "InsertCount": 10,
  "UseInsertTemplate": true,

  "ThemePath": "$baseDir/Configurations/Themes/Absolute.xaml",
  "StartupWidth": 1400,
  "StartupHeight": 600,
  "FrozenColumn": 1,
  "MinRowHeight": 25,
  "PreviewWidth": "*",
  "FontFamily": "Courier New,Roboto Mono",
  "PrimaryFontSize": 16,
  "SecondaryFontSize": 14
}